{
  "root": true,
  "compilerOptions": {
    "allowJs": true,
    "outDir": "./build/",
    "sourceMap": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "lib": ["es5", "es6", "dom"],
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "./env/*": ["./env/*"],
      "./rules/*": ["./rules/*"],
      "./utils/*": ["./utils/*"]
    }
  },
  "awesomeTypescriptLoaderOptions": {}
}
